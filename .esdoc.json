{
  "source": "./",
  "destination": "./.esdoc",
  "excludes": ["node_modules"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "manual": {
          "files": ["./.manual/usage.md"]
        },
        "undocumentIdentifier": {
          "enable": true
        },
        "unexportIdentifier": {
          "enable": true
        }
      }
    },
    { "name": "esdoc-ecmascript-proposal-plugin", "option": { "all": true } }
  ]
}
